{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "max-len": [1, 120, 2, {
            "ignoreComments": true
        }
       ],
       "no-console":0,
       "indent": [2],
       "no-tabs": 0,
       "no-underscore-dangle": 0,
       "react/prop-types": 0
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb-base"]
}
